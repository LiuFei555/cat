page {
  background-color: #EFEFEF;
}
.box {
  display: flex;
  flex-direction: column;
}
.box .users {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 750rpx;
  height: 146rpx;
  margin-bottom: 2rpx;
  margin-top: 1rpx;
  background-color: #FFFFFF;
}
.box .users .userid {
  display: flex;
  align-items: center;
  margin-left: 30rpx;
}
.box .users .userid .userHead {
  width: 86rpx;
  height: 86rpx;
  background-color: #DD524D;
  border-radius: 50%;
  margin-right: 20rpx;
}
.box .users .userid .userHead image {
  width: 86rpx;
  height: 86rpx;
}
.box .users .userid .userHead text {
  font-family: PingFangSC-Medium;
  font-size: 30rpx;
  color: #000000;
  letter-spacing: 0.05rpx;
  font-weight: 500;
}
.box .users .unfocus {
  display: flex;
  align-items: center;
  width: 172rpx;
  height: 56rpx;
  border: 1rpx solid #D4D4D4;
  border-radius: 28rpx;
  margin-right: 30rpx;
  font-family: PingFangSC-Medium;
  font-size: 26rpx;
  letter-spacing: 0.04rpx;
  font-weight: 500;
}
.box .users .unfocus image {
  width: 15.02rpx;
  height: 15.02rpx;
  margin-left: 20.49rpx;
  margin-right: 13.49rpx;
}

