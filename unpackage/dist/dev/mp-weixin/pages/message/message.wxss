.dian3 {
  width: 34rpx;
  height: 24.24rpx;
  background-color: #ff6969;
  border-radius: 12rpx;
  position: fixed;
  left: 382rpx;
  top: 1122.06rpx;
  z-index: -1;
}
.nav {
  position: relative;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: flex-end;
  border-bottom: 1rpx solid #efefef;
  background-color: #FFFFFF;
}
.nav .dian1, .nav .dian2 {
  font-family: PingFangSC-Semibold;
  font-size: 20rpx;
  color: #FFFFFF;
  letter-spacing: 0.03rpx;
  font-weight: 600;
  position: absolute;
  background-color: #ff6969;
  border-radius: 50rpx;
  line-height: 28rpx;
  text-align: center;
}
.nav .dian1 {
  top: 4rpx;
  left: 315rpx;
  width: 28rpx;
  height: 28rpx;
}
.nav .dian2 {
  width: 37rpx;
  height: 28rpx;
  border-radius: 14rpx;
  top: 4rpx;
  right: 218rpx;
}
.nav .remind, .nav .letters {
  text-align: center;
  font-family: PingFangSC-Medium;
  font-size: 30rpx;
  letter-spacing: 0;
  font-weight: 500;
}
.nav .remind view, .nav .letters view {
  width: 100rpx;
  height: 6rpx;
  border-radius: 4rpx;
  margin-top: 12rpx;
}
.nav .remind {
  margin-right: 80rpx;
}
.nav .checked {
  color: #81D195;
}
.nav .checked view {
  background-color: #81D195;
}
.nav .uncheck {
  color: #3F3F3F;
}
